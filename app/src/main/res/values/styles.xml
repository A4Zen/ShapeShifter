<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

    </style>

    <style name="SquareTheme" parent="AppTheme">
        <item name="android:drawable">@drawable/square_hidden</item>
    </style>

    <style name="TriangleTheme" parent="AppTheme">
        <item name="android:drawable">@drawable/triangle_hidden</item>
    </style>

    <style name="PentagonTheme" parent="AppTheme">
        <item name="android:drawable">@drawable/pentagon_hidden</item>
    </style>
</resources>
